$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalMoa").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_moa_id")) {
    document.getElementById("ppsp_moa_id").innerHTML = "<%= j(render 'form_field_moa') %>";
  }
  if (document.querySelector(".list-database")) {
    document.querySelector(".list-database").innerHTML = "<%= j(render 'list_moas') %>";
  }
})

