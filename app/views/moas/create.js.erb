$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalMoa").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_moa_id")) {
    document.getElementById("ppsp_moa_id").innerHTML = "<%= j(render 'form_field_moa') %>";
  }
  if (document.querySelector(".content-db")) {
    document.querySelector(".content-db").innerHTML = "<%= j(render 'database/content-db') %>";
  }
})

