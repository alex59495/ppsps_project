$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalMoa").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_moa_id")) {
    document.getElementById("ppsp_moa_id").innerHTML = "<%= j(render 'form_field_moa') %>";
  }
  if (document.querySelector(".flexbox-database")) {
    document.querySelector(".flexbox-database").innerHTML = "<%= j(render 'content-db') %>";
  }
})

