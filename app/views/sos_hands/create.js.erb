$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalSosHand").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_sos_hand_id")) {
    document.getElementById("ppsp_sos_hand_id").innerHTML = "<%= j(render 'form_field_sos_hand') %>";
  }
  if (document.querySelector(".flexbox-database")) {
    document.querySelector(".flexbox-database").innerHTML = "<%= j(render 'content-db') %>";
  }
})

