$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalHospital").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.querySelector(".ppsp_hospital_id")) {
    document.querySelector(".ppsp_hospital_id").innerHTML = "<%= j(render 'form_field_hospital') %>";
  }
  if (document.querySelector(".flexbox-database")) {
    document.querySelector(".flexbox-database").innerHTML = "<%= j(render 'content-db') %>";
  }
})

