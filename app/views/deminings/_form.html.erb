<%= simple_form_for @demining, remote: true do |f| %>
  <%= f.input :name, label: 'Nom' %>
  <%= f.input :address, label: 'Adresse' %>
  <%= f.input :phone, label: 'Téléphone' %>
  <%= f.button :button, 'Valider', class: 'btn-blue', id: "DeminingBtn", data: { disable_with: '<i class="fas fa-spin fa-spinner"></i> En cours...'.html_safe }%>
<% end %>