$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalDemining").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_demining_id")) {
    document.getElementById("ppsp_demining_id").innerHTML = "<%= j(render 'form_field_demining') %>";
  }
  if (document.querySelector(".container-database")) {
    document.querySelector(".container-database").innerHTML = "<%= j(render 'list_deminings') %>";
  }
})

