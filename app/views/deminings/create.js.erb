$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalDemining").hide();
  $(".fade.modal").hide();
  document.getElementById("new_demining").reset();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_demining_id")) {
    document.getElementById("ppsp_demining_id").innerHTML = "<%= j(render 'form_field_demining') %>";
  }
  if (document.querySelector(".content-db")) {
    document.querySelector(".content-db").innerHTML = "<%= j(render 'database/content-db') %>";
  }
})

