<%= simple_form_for @work_medecine, remote: true do |f| %>
  <%= f.input :address, label: 'Adresse' %>
  <%= f.input :phone, label: 'Téléphone' %>
  <%= f.input :fax, label: 'Fax' %>
  <%= f.button :button, 'Valider', class: 'btn-blue', id: "WorkMedecineBtn", data: { disable_with: '<i class="fas fa-spin fa-spinner"></i> En cours...'.html_safe } %>
<% end %>