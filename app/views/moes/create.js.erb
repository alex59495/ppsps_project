$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalMoe").hide();
  document.getElementById("new_moe").reset();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_moe_id")) {
    document.getElementById("ppsp_moe_id").innerHTML = "<%= j(render 'form_field_moe') %>";
  }
  if (document.querySelector(".content-db")) {
    document.querySelector(".content-db").innerHTML = "<%= j(render 'database/content-db') %>";
  }
})

