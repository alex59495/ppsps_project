$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalMoe").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_moe_id")) {
    document.getElementById("ppsp_moe_id").innerHTML = "<%= j(render 'form_field_moe') %>";
  }
  if (document.querySelector(".container-database")) {
    document.querySelector(".container-database").innerHTML = "<%= j(render 'list_moes') %>";
  }
})

