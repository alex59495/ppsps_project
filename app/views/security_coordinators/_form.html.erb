<%= simple_form_for @security_coordinator, remote: true do |f| %>
  <%= f.input :name, label: 'Nom' %>
  <%= f.input :representative, label: 'Représentant' %>
  <%= f.input :address, label: 'Adresse' %>
  <%= f.input :phone, label: 'Téléphone' %>
  <%= f.input :email, label: 'Email' %>
  <%= f.submit 'Ajouter', class: 'btn-blue', id: "SecurityBtn" %>
<% end %>