$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalSecurityCoordinator").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_security_coordinator_id")) {
    document.getElementById("ppsp_security_coordinator_id").innerHTML = "<%= j(render 'form_field_security_coordinator') %>";
  }
  if (document.querySelector(".content-db")) {
    document.querySelector(".content-db").innerHTML = "<%= j(render 'database/content-db') %>";
  }
})