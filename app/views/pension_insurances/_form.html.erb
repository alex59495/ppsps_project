<%= simple_form_for @pension_insurance, remote: true do |f| %>
  <%= f.input :address, label: 'Adresse' %>
  <%= f.input :phone, label: 'Téléphone' %>
  <%= f.input :fax, label: 'Fax' %>
  <%= f.input :website, label: 'Site internet' %>
  <%= f.button :button, "Valider", class: 'btn-blue', id: "PensionBtn", data: { disable_with: '<i class="fas fa-spin fa-spinner"></i> En cours...'.html_safe } %>
<% end %>