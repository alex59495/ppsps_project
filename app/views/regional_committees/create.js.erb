$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalRegionalCommittee").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.getElementById("ppsp_regional_committee_id")) {
    document.getElementById("ppsp_regional_committee_id").innerHTML = "<%= j(render 'form_field_regional_committee') %>";
  }
  if (document.querySelector(".content-db")) {
    document.querySelector(".content-db").innerHTML = "<%= j(render 'content-db') %>";
  }
})

