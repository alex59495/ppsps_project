<%= simple_form_for @regional_committee, remote: true do |f| %>
  <%= f.input :name, label: 'Nom' %>
  <%= f.input :address, label: 'Adresse'%>
  <%= f.input :phone, label: 'Téléphone' %>
  <%= f.input :fax, label: 'Fax' %>
  <%= f.input :website, label: 'Site internet' %>
  <%= f.button :button, 'Valider', class: 'btn-blue', id: "RegionalBtn", data: { disable_with: '<i class="fas fa-spin fa-spinner"></i> En cours...'.html_safe } %>
<% end %>