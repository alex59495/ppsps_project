<%= simple_form_for @regional_committee, remote: true do |f| %>
  <%= f.input :name, label: 'Nom' %>
  <%= f.input :address, label: 'Adresse'%>
  <%= f.input :phone, label: 'Téléphone' %>
  <%= f.input :fax, label: 'Fax' %>
  <%= f.input :website, label: 'Site internet' %>
  <%= f.submit 'Ajouter', class: 'btn-blue', id: "RegionalBtn" %>
<% end %>