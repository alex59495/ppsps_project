$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalAntiPoison").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.querySelector(".ppsp_anti_poison_id")) {
    document.querySelector(".ppsp_anti_poison_id").innerHTML = "<%= j(render 'form_field_anti_poison') %>";
  }
  if (document.querySelector(".flexbox-database")) {
    document.querySelector(".flexbox-database").innerHTML = "<%= j(render 'content-db') %>";
  }
})

