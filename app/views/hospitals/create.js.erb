$(document).ready(function() {
  $(".modal-backdrop").hide();
  $("#ModalHospital").hide();
  $(".modal-open").css({overflow: 'scroll'});
  if (document.querySelector(".ppsp_hospital_id")) {
    document.querySelector(".ppsp_hospital_id").innerHTML = "<%= j(render 'form_field_hospital') %>";
  }
  if (document.querySelector(".container-database")) {
    document.querySelector(".container-database").innerHTML = "<%= j(render 'list_hospitals') %>";
  }
})

